<section class="hbsBody">
    <main>
        <div>
            <h1>{{character.name}}</h1>
            <img src={{character.image}} alt={{character.name}} class="imgCls">
            <p class="para">{{{character.description}}}</p>
            <h2>Comics</h2>
            {{#each character.comics}}
                <ul class="content">
                    <li>{{this}}</li>
                </ul>
            {{/each}}
        </div>
    </main>
</section>